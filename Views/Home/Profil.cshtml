@model List<Proje.Models.siniflar.Biletler>

@{
    ViewData["Title"] = "Profil";
    Layout = "~/Views/Shared/_testlayout.cshtml";
}

<br />
<br />
<section class="w3l-grids-3 py-5">
    <div class="container py-md-5">
        <div class="title-content text-left mb-lg-5 mb-4">
            <h6 class="sub-title"> @_localization.GetKey("profil").Value</h6>
            <h3 class="hny-title"> @_localization.GetKey("bilet").Value</h3>
        </div>
        <div class="row bottom-ab-grids">
            <!--/row-grids-->
            @foreach (var bilet in Model)
            {
                <div class="col-lg-6 subject-card mt-lg-0 mt-4">
                    <div class="subject-card-header p-4">
                        <a class="card_title p-lg-4d-block">
                            <div class="row align-items-center">
                                <div class="col-sm-7 subject-content mt-sm-0 mt-4">
                                    <h4>@bilet.güzergah</h4>
                                    <p>@bilet.KullaniciTc</p>
                                    <p>@bilet.KullaniciEmail</p>
                                    <p>@bilet.KullaniciAdi</p>
                                    <p>@bilet.Gün</p>
                                    <p>@bilet.zaman</p>
                                    <p>Koltuk no:@bilet.koltuk</p>
                                </div>
                            </div>
                        </a>
                    </div>
                </div>
            }
        </div>
    </div>
</section>
